<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.chessio.chessio_client.SceneControllers.SettingsController">
    <children>
        <Label text="Choose Enemy Level (1-20)" />
        <Slider fx:id="levelSlider" min="1" max="20" value="1" blockIncrement="1" />
        <RadioButton fx:id="whiteRadioButton" text="White" />
        <RadioButton fx:id="blackRadioButton" text="Black" />
        <Button text="Start Game" onAction="#startGame" />
    </children>
</VBox>
